#include <stdio.h>
int main() {
    int num, i;
    printf("Enter the number of elements in the set: ");
    scanf("%d", &i);
    printf("Enter the elements of the set:\n");
    for(int j = 0; j < i; j++) {
        scanf("%d", &num);
        if(num % 2 == 0) {
            printf("%d is an even number.\n", num);
        } else {
            printf("%d is an odd number.\n", num);} 
            }
 return 0;
}
